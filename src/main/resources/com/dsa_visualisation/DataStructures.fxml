<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<VBox prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dsa_visualisation.DataStructuresController">
   <children>
      <GridPane hgap="20.0" vgap="20.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onArrayButtonClick" styleClass="button2" stylesheets="@styles.css">
               <graphic>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" stylesheets="@styles.css">
                     <children>
                        <FontIcon iconLiteral="fas-arrows-alt-h" iconSize="80" />
                        <Label text="Array">
                           <font>
                              <Font name="Segoe UI Semibold" size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </graphic>
            </Button>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onLinkedListButtonClick" styleClass="button2" stylesheets="@styles.css" GridPane.columnIndex="1">
               <graphic>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" stylesheets="@styles.css">
                     <children>
                        <FontIcon iconLiteral="fas-link" iconSize="75">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </FontIcon>
                        <Label graphicTextGap="0.0" text="Linked list">
                           <font>
                              <Font name="Segoe UI Semibold" size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </graphic>
            </Button>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onStackButtonClick" styleClass="button2" stylesheets="@styles.css" GridPane.columnIndex="2">
               <graphic>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" stylesheets="@styles.css">
                     <children>
                        <FontIcon iconLiteral="fas-layer-group" iconSize="75">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </FontIcon>
                        <Label graphicTextGap="0.0" text="Stack">
                           <font>
                              <Font name="Segoe UI Semibold" size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </graphic>
            </Button>
            <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onQueueButtonClick" styleClass="button2" stylesheets="@styles.css" GridPane.rowIndex="1">
               <graphic>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" stylesheets="@styles.css">
                     <children>
                        <FontIcon iconLiteral="fas-list-ul" iconSize="80" />
                        <Label text="Queue">
                           <font>
                              <Font name="Segoe UI Semibold" size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </graphic>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </GridPane>
   </children>
</VBox>
